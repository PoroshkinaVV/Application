<p id="notice"><%= notice %></p>

<h1>Listing Auditories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Building</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditories.each do |auditory| %>
      <tr>
        <td><%= auditory.name %></td>
        <td><%= auditory.building %></td>
        <td><%= link_to 'Show', auditory %></td>
        <td><%= link_to 'Edit', edit_auditory_path(auditory) %></td>
        <td><%= link_to 'Destroy', auditory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auditory', new_auditory_path %>
